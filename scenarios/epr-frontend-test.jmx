          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Crumb value Extractor PostProcessor" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Get cookie manager from the test plan
def cookieManager = sampler.getCookieManager()    

// If cookie manager is at thread group level
//if (cookieManager == null) {
  //  cookieManager = ctx.getThreadGroup().getCookieManager()
//}

def crumbValue = &quot;&quot;
if (cookieManager != null) {
    def cookies = cookieManager.getCookies()
    cookies.each { cookie -&gt;
      log.info(cookie.getName())
        if (cookie.getName() == &quot;crumb&quot;) {
        	def iterator = cookie.iterator()
        	while(iterator.hasNext()) {
        		def item = iterator.next()
        		if (item.getStringValue() == &quot;crumb&quot;) {
        			item = iterator.next()
        			crumbValue = item.getStringValue()
        			log.info(item.getStringValue())
        			break
        		}
        	}
        	
        }
    }
}

vars.put(&quot;crumb_value&quot;, crumbValue)
log.info(&quot;Crumb value: ${crumbValue}&quot;)</stringProp>
          </JSR223PostProcessor>
